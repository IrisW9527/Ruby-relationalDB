<div class="dir">
  <br/>
  <%= link_to('Profile', controller: "profiles", action: "index") %>
</div>

<head>
  <div class="hd">
      <h1>
        <%= current_user.username %>'s Photos 
      </h1>
  </div>
  <br/>
</head>

<body>
  <div class="pt-idx">
    
      <table class="tb1">
        <thead>
          <tr>
            <th>Title</th>
            <th>Image</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @photos.each do |photo| %>
            <tr>
              <td><%= photo.title %></td>
              <td><%= photo.image %></td>
              <td><%= link_to 'Show', photo %></td>
              <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
              <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>

      <br>
      <%= link_to 'New Photo', new_photo_path %>

  </div>

</body>



