<div class="lg-hd">
	<br/>
	<h1>Login</h1>
</div>

<body class="aa">
	<div class="ab">

		<%= form_with(model: User.new, url: sessions_path) do |f| %>
		
			<div class="field">
				<%=f.label :username%> <br/> <%=f.text_field :username%>
			</div>
			<div class="field">
				<%=f.label :password%> <br/> <%=f.password_field :password%>
			</div>

			<div class='actions'>
				<%=f.submit 'Login'%>
			</div>

		<%end%>
	</div>

</body>
